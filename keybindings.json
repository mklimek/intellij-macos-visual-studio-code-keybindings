// Place your key bindings in this file to overwrite the defaults
[
    { "key": "cmd+n",                 "command": "workbench.action.quickOpen" },
    { "key": "cmd+b",                 "command": "editor.action.goToDeclaration"},
    { "key": "cmd+alt+f7",            "command": "editor.action.referenceSearch.trigger"},
    { "key": "cmd+d",                 "command": "editor.action.copyLinesDownAction"},
    { "key": "shift+f10",             "command": "workbench.action.debug.start"},
    { "key": "f8",                    "command": "workbench.action.debug.stepOver"},
    { "key": "f7",                    "command": "workbench.action.debug.stepInto"},
    { "key": "f9",                    "command": "workbench.action.debug.continue"},
    { "key": "f10",                   "command": "workbench.action.debug.stepOut"},
    { "key": "shift+f6",              "command": "editor.action.changeAll"},
    { "key": "shift+cmd+down",        "command": "editor.action.moveLinesDownAction"},
    { "key": "shift+cmd+up",          "command": "editor.action.moveLinesUpAction"},
    { "key": "alt+cmd+left",          "command": "workbench.action.navigateBack" },
    { "key": "alt+cmd+rigt",          "command": "workbench.action.navigateForward" }
]